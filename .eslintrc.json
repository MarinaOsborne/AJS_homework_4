{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "linebreak-style": 0,
    "eqeqeq": 0,
    "global-require": 0,
    "consistent-return": 0,
    "import/extensions": 0,
    "max-len": 0
  }
}